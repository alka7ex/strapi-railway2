{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "altthumbnail": {
      "type": "string",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "descriptions": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
